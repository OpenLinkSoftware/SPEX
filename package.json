{
  "name": "@zazuko/spex",
  "version": "0.1.0",
  "author": "Zazuko (https://zazuko.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/zazuko/spex.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --formats umd,umd-min --inline-vue --name spex ./src/index.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "deploy": "./scripts/deploy.sh",
    "prepublish": "npm run build-lib"
  },
  "main": "dist/spex.umd.js",
  "files": [
    "/src",
    "/dist/spex.css",
    "/dist/spex.umd.min.js"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@rdfjs-elements/rdf-editor": "^0.3.7",
    "@rdfjs-elements/rdf-snippet": "^0.2.1",
    "@rdfjs/serializer-jsonld-ext": "^2.0.0",
    "@rdfjs/term-set": "1.0.1",
    "@tpluscode/rdf-ns-builders": "^0.4.2",
    "@tpluscode/rdf-string": "0.2.23",
    "@zazuko/rdf-vocabularies": "2021.3.31",
    "buefy": "^0.9.5",
    "clownface": "^1.2.0",
    "core-js": "^3.9.1",
    "d3": "6.6.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "4.0.8",
    "n3": "^1.9.0",
    "rdf-ext": "^1.3.1",
    "register-service-worker": "^1.7.2",
    "sparql-http-client": "2.2.3",
    "splitpanes": "2.3.6",
    "vue": "^2.6.12",
    "vue-panzoom": "^1.1.4",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.12",
    "@vue/cli-plugin-eslint": "~4.5.12",
    "@vue/cli-plugin-pwa": "~4.5.12",
    "@vue/cli-plugin-router": "~4.5.12",
    "@vue/cli-plugin-unit-jest": "~4.5.12",
    "@vue/cli-service": "~4.5.12",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.1.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "warn",
      "padded-blocks": "warn",
      "comma-dangle": "off"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  },
  "license": "MIT"
}
